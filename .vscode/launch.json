{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/user-auth/src/server.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Frontend Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/front/server.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch OPENAI Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/openAi/interactions.model.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch PROXY Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/queryHandler/interactions.model.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch userProfileHandler Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/userProfileHandler/interactions.model.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch databaseTechnologiesFilter Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/microservices/technologiesFilter/interactions.model.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG_MODE": "true"
            }
        }
    ],
    "compounds": [
        {
            "name": "Launch All Servers",
            "configurations": [
                "Launch Program",
                "Launch Frontend Server",
                "Launch OPENAI Server",
                "Launch PROXY Server",
                "Launch userProfileHandler Server",
                "Launch databaseTechnologiesFilter Server"
            ]
        }
    ]
}
